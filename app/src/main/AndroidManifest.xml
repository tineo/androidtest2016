<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="markerlab.debra">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icono_loli"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="markerlab.debra.usuario.UsuarioActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="markerlab.debra.usuariodetail.UsuarioDetailActivity"
            android:label="@string/title_activity_lawyer_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="markerlab.debra.addeditUsuario.AddEditUsuarioActivity"
            android:label="@string/title_activity_add_edit"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>